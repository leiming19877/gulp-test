{{~it :page}}
    <div class="swiper-slide">
        <ul>
            {{~page :d}}
                    <li class="f-cb">
                        <div class="text">
                             <p class="desc">{{=d.brandNameDesc}} {{=d.textureDesc}}*{{=d.specification}}</p>
                        </div>
                        <a class="u-shop-car" data-brandId="{{=d.brandName}}" data-textureId="{{=d.texture}}" data-specification="{{=d.specification}}" ></a>
                    </li>
           {{~}}
        </ul>
    </div>
{{~}}